{
	"compilerOptions": {
		"jsx": "react",
		"lib": [ "es2017", "dom" ],
		"module": "esnext",
		"moduleResolution": "node",
		"noImplicitAny": false,
		"outDir": "./dist/",
		"preserveConstEnums": true,
		"removeComments": true,
		"sourceMap": true,
		"target": "es2015"
	},
	"exclude": [
		"node_modules"
	]
}
